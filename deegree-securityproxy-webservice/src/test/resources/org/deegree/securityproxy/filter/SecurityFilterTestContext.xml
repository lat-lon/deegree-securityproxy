<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
  http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

  <bean id="proxyReportLoggerMock" class="org.mockito.Mockito" factory-method="mock">
    <constructor-arg value="org.deegree.securityproxy.logger.SecurityRequestResposeLogger" />
  </bean>

  <bean id="requestAuthorizationManagerMock" class="org.deegree.securityproxy.authorization.wcs.TestWcsRequestAuthorizationManager"  />
  
  <bean id="wcsRequestParser" class="org.mockito.Mockito" factory-method="mock">
    <constructor-arg value="org.deegree.securityproxy.request.WcsRequestParser" />
  </bean>

  <bean name="securityFilter" class="org.deegree.securityproxy.filter.SecurityFilter" />

</beans>
